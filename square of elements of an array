#include<stdio.h>
int main()
{
    int a[20],i,size,b[20];
    int sum=0;
    printf("enter the size of array\n");
    scanf("%d",&size);
    printf("enter the array elements\n");
    for(i=0;i<size;i++)
    {
        scanf("%d",&a[i]);
        b[i]=a[i]*a[i];
        printf("squares of matrix B obtained from A:%d \n",b[i]); 

     
    }
    
}
